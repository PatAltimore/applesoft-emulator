<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <!-- Copy disk folder contents to build output so LOAD/CATALOG work from any platform -->
  <ItemGroup>
    <None Include="disk\**" CopyToOutputDirectory="PreserveNewest" LinkBase="disk" />
  </ItemGroup>

</Project>
